  },
  "author": "Paris Time Machine",
  "license": "MIT",
  "dependencies": {
    "@gouvfr/dsfr": "^1.9.0",
    "leaflet-geosearch": "^3.8.0",
    "node-fetch": "^2.6.11"
  },
  "devDependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "concurrently": "^8.2.0"
  }
}

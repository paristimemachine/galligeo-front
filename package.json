{
  "name": "galligeo-front",
  "version": "1.0.0",
  "description": "Interface de géoréférencement pour cartes historiques avec intégration Gallica",
  "main": "index.html",
  "scripts": {
    "build-version": "node scripts/build-version.js",
    "version-info": "node scripts/version-info.js",
    "dev": "npm run build-version && echo 'Version générée avec succès'",
    "start": "npm run build-version",
    "mock-api": "node mock-api-server.js",
    "dev-with-api": "concurrently \"npm run mock-api\" \"npm run dev\""
  },
  "repository": {
    "type": "git",
